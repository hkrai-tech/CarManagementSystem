<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DeleteCartItem</servlet-name>
  	<display-name>DeleteCartItem</display-name>
  	<description></description>
  	<servlet-class>com.trishul.IndividualItem.DeleteCartItem</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>IncreaseStock</servlet-name>
  	<display-name>IncreaseStock</display-name>
  	<description></description>
  	<servlet-class>com.trishul.IndividualItem.IncreaseStock</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DeleteCartItem</servlet-name>
  	<url-pattern>/DeleteCartItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>IncreaseStock</servlet-name>
  	<url-pattern>/IncreaseStock</url-pattern>
  </servlet-mapping>
</web-app>
